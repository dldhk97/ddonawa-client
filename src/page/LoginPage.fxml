<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="379.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPageController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="또나와" textAlignment="CENTER" wrappingWidth="200.0">
         <font>
            <Font size="42.0" />
         </font>
      </Text>
      <HBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="아이디" textAlignment="CENTER" wrappingWidth="60.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Text>
            <TextField fx:id="idField" onKeyTyped="#On_idFiled_Typed" prefHeight="23.0" prefWidth="500.0" />
         </children>
         <padding>
            <Insets top="40.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="비밀번호" textAlignment="CENTER" wrappingWidth="60.0" />
            <PasswordField fx:id="pwField" onKeyTyped="#On_pwField_Typed" prefHeight="23.0" prefWidth="500.0" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </HBox>
      <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#On_loginBtn_Clicked" text="Login">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Button>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="RegisterBtn" mnemonicParsing="false" onAction="#OnRegisterBtnClicked" text="회원가입">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
