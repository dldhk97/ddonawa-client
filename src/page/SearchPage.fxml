         </children>
      </HBox>
      <TableView fx:id="table" onMouseClicked="#OnProductClicked" prefHeight="280.0" prefWidth="545.0">
        <columns>
          <TableColumn fx:id="ImageColumn" prefWidth="75.0" text="(썸네일)" />
          <TableColumn fx:id="ProductNameColumn" prefWidth="394.0" text="상품명" />
            <TableColumn fx:id="PriceColumn" minWidth="0.0" prefWidth="75.0" text="가격" />
        </columns>
      </TableView>
      <Button fx:id="testBtn" mnemonicParsing="false" onAction="#test" text="Button" />
   </children>
</FlowPane>
